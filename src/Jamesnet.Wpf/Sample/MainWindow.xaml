<Window
    x:Class="IconSamples.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:james="https://jamesnet.dev/xaml/presentation"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="parent"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <james:JamesPanel
        Background="Beige"
        Justify="SpaceAround"
        Orientation="Horizontal">
        <james:JamesPanel.Resources>
            <Style TargetType="{x:Type james:JamesIcon}">
                <Setter Property="Background" Value="Blue" />
            </Style>
        </james:JamesPanel.Resources>
        <james:JamesIcon Icon="AccountGroup" />
        <james:JamesIcon Icon="PokerChip" />
        <james:JamesIcon Icon="Creditcardchip" />
        <james:JamesIcon Icon="CreditcardchipOutline" />
        <james:JamesIcon Icon="Memory" />
        <james:JamesIcon Icon="Account" />
        <james:JamesIcon Icon="HomeCircle" />
        <james:JamesIcon Icon="HomeCircleOutline" />
        <james:JamesIcon Icon="Cash" />
        <james:JamesIcon Icon="Cash100" />
        <james:JamesIcon Icon="CashMulti" />
    </james:JamesPanel>
</Window>
